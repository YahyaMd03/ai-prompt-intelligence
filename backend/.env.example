# Backend env: copy to backend/.env and set values.
# Docker Compose: use host `db`. Local dev with remote Postgres: use your provider's connection string.
DATABASE_URL=postgresql://postgres:postgres@db:5432/prompt_intelligence
FRONTEND_ORIGIN=http://localhost:5173
GROQ_API_KEY=
GROQ_MODEL=llama-3.3-70b-versatile
GROQ_TIMEOUT_SECONDS=15
LOG_LEVEL=INFO
# Set to true to reject prompts containing obvious injection phrases (e.g. "forget instructions")
ENABLE_PROMPT_GUARD=false
